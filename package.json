{
  "name": "@things-factory/layout-ui",
  "version": "0.0.32",
  "main": "dist-server/index.js",
  "browser": "client/index.js",
  "things-factory": true,
  "license": "MIT",
  "author": "heartyoh",
  "scripts": {
    "serve": "things-factory",
    "serve:dev": "npm run clean && tsc && things-factory-dev",
    "build:client": "webpack --config node_modules/@things-factory/shell/webpack.config.js",
    "build:server": "tsc",
    "clean": "rm -rf dist-server dist-client",
    "prepublish": "npm run clean && npm run build:server",
    "migration": "things-factory-migration",
    "migration:create": "node ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "node ./node_modules/typeorm/cli.js migration:generate",
    "migration:run": "node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "node ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@things-factory/layout-base": "*",
    "@things-factory/shell": "*"
  },
  "devDependencies": {
    "@things-factory/auth-provider-session": "*",
    "@things-factory/auth-ui-session": "*",
    "@things-factory/board-ui": "*",
    "@things-factory/i18n-ui": "*",
    "@things-factory/menu-provider": "*",
    "@things-factory/menu-ui": "*",
    "@things-factory/more-ui": "*",
    "@things-factory/offline-ui": "*",
    "@things-factory/print-ui": "*",
    "@things-factory/resource-ui-mobile": "*",
    "@things-factory/setting-ui": "*",
    "@things-factory/system-ui": "*",
    "@things-factory/tester": "*"
  }
}
